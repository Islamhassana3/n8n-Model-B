{
  "name": "@makafeli/n8n-workflow-builder",
  "version": "0.8.0",
  "description": "Model Context Protocol server for n8n workflow management",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc",
    "dev": "tsc -w",
    "start": "node build/index.js",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit"
  },
  "bin": {
    "n8n-workflow-builder": "build/index.js"
  },
  "files": [
    "build/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "n8n",
    "workflow",
    "automation",
    "server"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1",
    "axios": "^1.7.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
